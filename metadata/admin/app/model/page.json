{
    "labels": {
        "name": {
            "en": "Pages",
            "fr": "Pages"
        },
        "singular_name": {
            "en": "Page",
            "fr": "Page"
        },
        "create_item": {
            "en": "Create Page",
            "fr": "Cr√©er une page"
        },
        "new_item": {
            "en": "New Page",
            "fr": "Nouvelle page"
        },
        "view_item": {
            "en": "View Page",
            "fr": "Voir la page"
        },
        "edit_item": {
            "en": "Edit Page",
            "fr": "Modifier la page"
        },
        "all_items": {
            "en": "All Pages",
            "fr": "Toutes les pages"
        },
        "menu_items": {
            "en": "Pages",
            "fr": "Pages"
        }
    },
    "properties": {
        "templateOptions": {
            "input_type": "charcoal/admin/property/input/structure-widget",
            "show_label": false,
            "widget_data": {
                "type": "charcoal/admin/widget/form-group/template-options",
                "display": "seamless",
                "show_empty": false
            },
            "structure_interfaces": [],
            "structure_metadata": {}
        }
    },
    "admin": {
        "lists": {
            "app.default": {
                "properties": [
                    "template_ident",
                    "title",
                    "last_modified",
                    "active"
                ],
                "orders": [
                    {
                        "property": "position",
                        "mode": "asc"
                    }
                ],
                "list_actions": [
                    {
                        "ident": "create",
                        "url": "object/edit?obj_type={{ objType }}",
                        "permissions": [ "object/create" ]
                    }
                ],
                "object_actions": {
                    "edit": {
                        "label": {
                            "en": "Modify",
                            "fr": "Modifier"
                        },
                        "url": "object/edit?obj_type={{obj_type}}&obj_id={{id}}",
                        "priority": 1,
                        "icon": "edit",
                        "actions": {
                            "goto": {
                                "active": false
                            },
                            "clone": {
                                "active": false
                            },
                            "delete": {
                                "active": false
                            }
                        }
                    },
                    "goto": {
                        "actionType": "seamless",
                        "condition": "isActiveRoute",
                        "label": {
                            "en": "Go to page",
                            "fr": "Voir la page"
                        },
                        "icon": "external-link",
                        "url": "{{baseUrl}}{{url}}",
                        "target": "_blank",
                        "priority": 2
                    }
                },
                "properties_options": {
                    "title": {
                        "view_options": {
                            "attr": {
                                "width": "55%"
                            }
                        }
                    },
                    "template_ident": {
                        "view_options": {
                            "attr": {
                                "width": "15%"
                            }
                        }
                    },
                    "last_modified": {
                        "view_options": {
                            "attr": {
                                "width": "15%"
                            }
                        }
                    },
                    "active": {
                        "view_options": {
                            "attr": {
                                "width": "5%",
                                "class": "text-center"
                            }
                        }
                    }
                }
            }
        },
        "default_list": "app.default",
        "forms": {
            "app.create": {
                "groups": {
                    "app/model/info": {
                        "priority": 100
                    },
                    "app/model/dev": {
                        "priority": 200
                    }
                },
                "sidebars": {
                    "default": {}
                }
            },
            "app.edit": {
                "groups": {
                    "app/model/info": {
                        "priority": 100
                    },
                    "app/model/advanced": {
                        "priority": 300
                    },
                    "app/model/dev": {
                        "priority": 400
                    },
                    "app/model/web": {
                        "priority": 500
                    },
                    "app/model/meta": {
                        "priority": 600
                    }
                },
                "sidebars": {
                    "default": {}
                }
            },
            "app.edit.home": {
                "groups": {
                    "app/model/info": {
                        "priority": 100
                    },
                    "app/model/template-options": {
                        "priority": 150
                    },
                    "app/model/advanced": {
                        "priority": 300
                    },
                    "app/model/dev": {
                        "priority": 400
                    },
                    "app/model/web": {
                        "priority": 500
                    },
                    "app/model/meta": {
                        "priority": 600
                    }
                },
                "sidebars": {
                    "default": {}
                }
            },
            "app.edit.technology": {
                "groups": {
                    "app/model/info": {
                        "priority": 100
                    },
                    "app/model/template-options": {
                        "priority": 150
                    },
                    "app/model/advanced": {
                        "priority": 300
                    },
                    "app/model/dev": {
                        "priority": 400
                    },
                    "app/model/web": {
                        "priority": 500
                    },
                    "app/model/meta": {
                        "priority": 600
                    }
                },
                "sidebars": {
                    "default": {}
                }
            }
        },
        "form_sidebars": {
            "default": {
                "properties": [
                    "created",
                    "created_by",
                    "last_modified",
                    "last_modified_by"
                ]
            }
        },
        "form_groups": {
            "app/model/info": {
                "title": "Info",
                "show_title": false,
                "show_header": false,
                "icon": "info",
                "properties": [
                    "title"
                ]
            },
            "app/model/template-options": {
                "icon": "cog",
                "title": "Content",
                "show_header": false,
                "show_title": true,
                "properties": [
                    "templateOptions"
                ]
            },
            "app/model/advanced": {
                "title": "Options",
                "show_title": false,
                "show_header": false,
                "icon": "cog",
                "properties": [
                    "master",
                    "slug",
                    "active"
                ]
            },
            "app/model/dev": {
                "title": "Dev",
                "show_title": false,
                "show_header": false,
                "icon": "code",
                "tabCssClasses": "pull-right",
                "permissions": ["dev_tab"],
                "properties": [
                    "template_ident",
                    "locked"
                ]
            },
            "app/model/meta": {
                "properties": [
                    "id",
                    "lastModified",
                    "lastModifiedBy",
                    "created",
                    "createdBy"
                ],
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] },
                        { "columns": [ 1, 1 ] },
                        { "columns": [ 1, 1 ] }
                    ]
                }
            }
        },
        "dashboards": {
            "admin.edit": {
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "form": {
                        "type": "charcoal/admin/widget/object-form",
                        "form_ident": "{{ formIdent }}",
                        "group_display_mode": "tab"
                    }
                }
            },
            "admin.table": {
                "layout": {
                    "structure": [
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "table": {
                        "type": "charcoal/admin/widget/table",
                        "obj_type": "app/model/page",
                        "collection_ident": "app.default",
                        "sortable": true
                    }
                }
            }
        },
        "default_create_dashboard": "admin.edit",
        "default_edit_dashboard": "admin.edit",
        "default_collection_dashboard": "admin.table"
    }
}
