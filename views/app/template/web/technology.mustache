{{> app/template/web/partial/site-header }}

<div class="o-scroll u-background-blue" data-module-scroll="main">
    <section data-scroll-section>
        <header class="c-header -white">
            {{> app/template/web/partial/header }}
        </header>
        <div class="c-section ">
            <div class="o-container">
                <div class="c-header-multi">
                    <h1 class="c-header-multi_title c-heading -h2 u-anim-text-line" data-module-split data-scroll
                        data-scroll-speed="2" data-scroll-position="top">
                        {{#_t}}technology.title{{/_t}}
                    </h1>
                    <p class="c-header-multi_content c-heading -h4 u-anim -delay-3">
                        {{#_t}}technology.subtitle{{/_t}}
                    </p>
                    <a href="#learn-more" class="c-button -white -hover-gray u-anim -delay-4" data-scroll-to>
                        <span class="c-button_label">
                            {{#_t}}site.learn-more{{/_t}}
                        </span>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <div data-scroll-section>
        <div class="c-section -no-padding-top">
            <div class="o-container">
                <div class="c-video || o-ratio || u-16:9 || u-border-radius" data-module-video data-scroll
                    data-scroll-repeat data-scroll-call="toggle,Video" data-module-video-modal-toggler
                    data-video-modal-toggler="toggler" data-video-modal-toggler-host="vimeo"
                    data-video-modal-toggler-id="386905142">
                    <button class="c-video_button" aria-label="play" type="button">
                        <span class="c-video_button_icon">
                            <svg role="img" aria-hidden="true">
                                <use href="assets/app/images/sprite.svg#play"></use>
                            </svg>
                        </span>
                    </button>

                    <video src="{{# withBaseUrl }}assets/app/video/intro.mp4{{/ withBaseUrl }}" autoplay="false"
                        muted="true" loop="true" data-video="video">
                    </video>
                </div>
            </div>
        </div>
    </div>

    <div data-scroll-section>
        <div class="c-section -no-padding-top" id="learn-more">
            <div class="o-container">
                <div class="o-layout">
                    <div class="o-layout_item u-4/5@from-small">
                        <div class="c-wysiwyg u-anim">
                            <p class="c-heading -h3 u-anim-text-line" data-scroll data-module-split>{{introExcerpt}}</p>
                        </div>
                    </div>
                </div>
                <div class="o-layout -gutter-huge u-text-right">
                    <div class="o-layout_item u-1/3@from-small u-text-left">
                        <div class="c-wysiwyg u-anim" data-scroll data-scroll-offset="5%">
                            <p>{{introText1}}</p>
                        </div>
                    </div>
                    <div class="o-layout_item u-1/3@from-small u-text-left">
                        <div class="c-wysiwyg u-anim" data-scroll data-scroll-offset="15%">
                            <p>{{introText2}}</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div data-scroll-section id="meet-the-team">
        <div class="c-section">
            <div class="o-container c-team u-relative">
                <div class="u-relative">
                    <p class="c-splash-words -absolute">
                        {{# isFrench }}
                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset" data-scroll
                                data-scroll-offset="10%">Rencontrez</span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item " data-scroll data-scroll-offset="10%">l'Équipe
                            </span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset-small" data-scroll
                                data-scroll-offset="10%">Derrière
                            </span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset" data-scroll
                                data-scroll-offset="10%">Le</span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset-small" data-scroll
                                data-scroll-offset="10%">Projet</span>
                        </span>
                        {{/ isFrench }}
                        {{^ isFrench }}
                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset" data-scroll
                                data-scroll-offset="10%">Meet</span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item " data-scroll data-scroll-offset="10%">The Team
                            </span>
                        </span>

                        <span class="c-splash-words_row">
                            <span class="c-splash-words_row_item -offset-small" data-scroll
